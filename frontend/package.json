{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "start:dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "prepare": "cd ../ && husky install ./frontend/.husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:e2e": "cypress open",
    "test:e2e:ci": "wait-on http://localhost:5000/api/v1 && wait-on http://localhost:3000/ && cypress run",
    "wait-on": "wait-on"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --project tsconfig.json --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write"
    ],
    "*.{html,json,js,md,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "axios": "^0.22.0",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "generated-api": "file:src/open-api",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.24.4",
    "sass": "^1.42.1",
    "ts-node": "^10.4.0",
    "vercel-is-pull-request": "0.0.8",
    "yup": "^0.32.9",
    "zustand": "^3.5.12"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/react": "17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "^27.3.1",
    "cypress": "^8.5.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "tslint": "^6.1.3",
    "typescript": "4.4.3",
    "wait-on": "^6.0.0"
  }
}
