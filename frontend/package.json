{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "start:dev": "next dev",
    "start:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "lint": "next lint",
    "prepare": "cd ../ && husky install ./frontend/.husky",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:ci": "jest --passWithNoTests --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:e2e": "cypress open",
    "test:e2e:ci": "wait-on http://localhost:5001/api/v1 && wait-on http://localhost:3000/ && cypress run",
    "wait-on": "wait-on"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --project tsconfig.json --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "prettier --write"
    ],
    "*.{html,json,js,md,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@tanstack/react-query": "^4.19.1",
    "@tanstack/react-query-devtools": "^4.19.1",
    "axios": "^1.2.1",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "framer-motion": "^7.6.19",
    "generated-api": "file:src/open-api",
    "next": "^13.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.56.2",
    "ts-node": "^10.9.1",
    "vercel-is-pull-request": "0.0.8",
    "yup": "^0.32.11",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-jest": "^29.3.1",
    "cypress": "^12.0.2",
    "eslint": "8.29.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "react-test-renderer": "^18.2.0",
    "tslint": "^6.1.3",
    "typescript": "4.9.4",
    "wait-on": "^6.0.1"
  }
}
